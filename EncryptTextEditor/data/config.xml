<?xml version="1.0" encoding="utf-8"?>
<config>
  <!--程序运行相关-->
  <app>
    <!--记录程序是否首次启动-->
    <firstStart>false</firstStart>
    <!--主窗口的宽度-->
    <width>900</width>
    <!--主窗口高度-->
    <height>549</height>
    <!--窗口出现的x坐标-->
    <x>666</x>
    <!--窗口出现的y坐标-->
    <y>304</y>
  </app>
  <!--程序主题样式-->
  <style>
    <!--文本区域的字体-->
    <font>
      <name>微软雅黑</name>
      <size>12</size>
      <!--如果regular为true的话，就不检查以下4个属性了-->
      <regular>true</regular>
      <bold>false</bold>
      <italic>false</italic>
      <strikeout>false</strikeout>
      <underline>false</underline>
    </font>
    <foreColor>#ffffff</foreColor>
    <backColor>#293134</backColor>
  </style>
  <!--适配扩展名-->
  <extensions>
    <extension>
      <name>*.ept</name>
      <desc>加密文本文件</desc>
    </extension>
    <extension>
      <name>*.act</name>
      <desc>账户文件</desc>
    </extension>
  </extensions>
</config>